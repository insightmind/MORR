{
  "name": "MORR Browser Extension",
  "version": "0.0.1",
  "description": "The MORR browser extension",
  "permissions": ["<all_urls>", "activeTab", "storage", "tabs", "webNavigation", "downloads"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_title": "MORR Browser Extension"
  },
  "manifest_version": 2
}