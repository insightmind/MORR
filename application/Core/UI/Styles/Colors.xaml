<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Dialog.Button.Foreground" Color="#FFF4F4F5" />
    <SolidColorBrush x:Key="Dialog.Button.Background" Color="#FF636869" />
    <SolidColorBrush x:Key="Dialog.Button.Background.Hover" Color="#FF3F3F41" />
    <SolidColorBrush x:Key="Dialog.Button.Background.Pressed" Color="#FF007ACC" />

    <SolidColorBrush x:Key="Dialog.Border.Active" Color="#FF007ACC" />
    <SolidColorBrush x:Key="Dialog.Background" Color="#FF2D2D30" />
    <SolidColorBrush x:Key="Dialog.Foreground" Color="#FFF4F4F5" />
    <SolidColorBrush x:Key="Dialog.Title.Foreground.Active" Color="#FF999988" />
    <SolidColorBrush x:Key="Dialog.Title.Foreground.Inactive" Color="#FF707070" />
</ResourceDictionary>